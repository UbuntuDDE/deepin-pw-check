#: lib/deepin_pw_check.c:545
msgid "Invalid error type"
msgstr "Neveljavna vrsta napake"

#: lib/deepin_pw_check.c:555
msgid "Checking successful"
msgstr "Preverjanje je bilo uspešno"

#: lib/deepin_pw_check.c:557
msgid "The password cannot be empty"
msgstr "Geslo ne more biti prazno"

#: lib/deepin_pw_check.c:561
#, c-format
msgid "Password must have at least %d characters"
msgstr "Geslo mora imeti vsaj %d znakov"

#: lib/deepin_pw_check.c:566
#, c-format
msgid "Password must be no more than %d characters"
msgstr "Geslo ne sme imeti več kot %d znakov"

#: lib/deepin_pw_check.c:570
#, c-format
msgid "Password can only contain %s"
msgstr "Geslo lahko vsebuje le %s"

#: lib/deepin_pw_check.c:577
#, c-format
msgid "Password must not contain more than %d palindrome characters"
msgstr "Geslo ne sme vsebovati več kot %d palindromne znake"

#: lib/deepin_pw_check.c:581 lib/deepin_pw_check.c:583
#: lib/deepin_pw_check.c:585
msgid "Create a strong password please"
msgstr "Prosimo, da ustvarite močno geslo"

#: lib/deepin_pw_check.c:587
msgid "Do not use common words and combinations as password"
msgstr "Za geslo ne uporabljajte pogostih besed in fraz"

#: lib/deepin_pw_check.c:589
msgid "Parameter options are invalid"
msgstr "Neveljavne opcije parametra"

#: lib/deepin_pw_check.c:591
msgid "Internal error"
msgstr "Notranja napaka"

#: lib/deepin_pw_check.c:593
msgid "Invalid user"
msgstr "Napačen uporabnik"

#: lib/deepin_pw_check.c:595
msgid "It does not meet password rules"
msgstr "Ne ustreza pravilom za geslo"

